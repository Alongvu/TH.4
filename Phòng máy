        System.out.print("Nhập tình trạng máy: ");
        donGia = scanner.nextLine();
        System.out.print("Nhập số lượng: ");
        soLuong = scanner.nextInt();
    }
     
    @Override
    public String toString() {
        return "Mã máy: " + this.maMay+ ", tên nhà xuất bản: " + this.nhaXuatBan + ", tình trạng máy: " + 
                this.tinhTrangMay + ", số lượng: " + this.soLuong;
    }
}
